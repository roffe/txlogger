<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<assembly xmlns="urn:schemas-microsoft-com:asm.v1" manifestVersion="1.0">
    <assemblyIdentity
        version="9.0.0.0"
        processorArchitecture="x86"
        name="txlogger"
        type="win32"
    />
    <description>data logging for automobile use</description>
    <trustInfo xmlns="urn:schemas-microsoft-com:asm.v3">
        <security>
            <requestedPrivileges>
                <requestedExecutionLevel level="requireAdministrator" uiAccess="false" />
            </requestedPrivileges>
        </security>
    </trustInfo>
    <fileTypeAssociation xmlns="http://schemas.microsoft.com/appx/2013/manifest">
        <name>Text File</name>
        <extensions>
            <extension>.txt</extension>
        </extensions>
        <supportedFileTypes>
            <fileType>.txt</fileType>
        </supportedFileTypes>
    </fileTypeAssociation>
</assembly>